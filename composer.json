{
    "name": "drdelay/pokemon-go",
    "description": "Pokemon Go API Library",
    "type": "library",
    "keywords": [
        "DrDelay",
        "pokemon-go"
    ],
    "homepage": "https://github.com/DrDelay/pokemon-go",
    "license": "MIT",
    "authors": [
        {
            "name": "DrDelay",
            "email": "info@vi0lation.de",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "psr/log": "^1.0",
        "league/container": "^2.2",
        "guzzlehttp/guzzle": "^6.2",
        "psr/cache": "^1.0",
        "fig/cache-util": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "DrDelay\\PokemonGo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DrDelay\\PokemonGo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "config": {
        "sort-packages": true
    }
}
